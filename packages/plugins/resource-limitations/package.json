{
  "name": "@envelop/resource-limitations",
  "version": "6.1.3",
  "type": "module",
  "description": "A rate-limit implementation based on resource limitations and static calculation of the score (similar to GitHub GraphQL API)",
  "repository": {
    "type": "git",
    "url": "https://github.com/graphql-hive/envelop.git",
    "directory": "packages/plugins/resource-limitations"
  },
  "author": "Laurin Quast <laurinquast@googlemail.com>",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  },
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "exports": {
    ".": {
      "require": {
        "types": "./dist/typings/index.d.cts",
        "default": "./dist/cjs/index.js"
      },
      "import": {
        "types": "./dist/typings/index.d.ts",
        "default": "./dist/esm/index.js"
      },
      "default": {
        "types": "./dist/typings/index.d.ts",
        "default": "./dist/esm/index.js"
      }
    },
    "./*": {
      "require": {
        "types": "./dist/typings/*.d.cts",
        "default": "./dist/cjs/*.js"
      },
      "import": {
        "types": "./dist/typings/*.d.ts",
        "default": "./dist/esm/*.js"
      },
      "default": {
        "types": "./dist/typings/*.d.ts",
        "default": "./dist/esm/*.js"
      }
    },
    "./package.json": "./package.json"
  },
  "typings": "dist/typings/index.d.ts",
  "peerDependencies": {
    "@envelop/core": "workspace:^",
    "graphql": "^14.0.0 || ^15.0.0 || ^16.0.0"
  },
  "dependencies": {
    "@envelop/extended-validation": "workspace:^",
    "@graphql-tools/utils": "^10.0.0",
    "tslib": "^2.5.0"
  },
  "devDependencies": {
    "@envelop/core": "workspace:^",
    "@graphql-tools/schema": "10.0.23",
    "graphql": "16.10.0",
    "typescript": "5.8.2"
  },
  "publishConfig": {
    "directory": "dist",
    "access": "public"
  },
  "sideEffects": false,
  "buildOptions": {
    "input": "./src/index.ts"
  },
  "typescript": {
    "definition": "dist/typings/index.d.ts"
  }
}

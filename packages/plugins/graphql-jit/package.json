{
  "name": "@envelop/graphql-jit",
  "version": "4.4.2",
  "author": "Dotan Simha <dotansimha@gmail.com>",
  "license": "MIT",
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/n1ru4l/envelop.git",
    "directory": "packages/plugins/graphql-jit"
  },
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "exports": {
    ".": {
      "require": {
        "types": "./dist/typings/index.d.cts",
        "default": "./dist/cjs/index.js"
      },
      "import": {
        "types": "./dist/typings/index.d.ts",
        "default": "./dist/esm/index.js"
      },
      "default": {
        "types": "./dist/typings/index.d.ts",
        "default": "./dist/esm/index.js"
      }
    },
    "./*": {
      "require": {
        "types": "./dist/typings/*.d.cts",
        "default": "./dist/cjs/*.js"
      },
      "import": {
        "types": "./dist/typings/*.d.ts",
        "default": "./dist/esm/*.js"
      },
      "default": {
        "types": "./dist/typings/*.d.ts",
        "default": "./dist/esm/*.js"
      }
    },
    "./package.json": "./package.json"
  },
  "typings": "dist/typings/index.d.ts",
  "typescript": {
    "definition": "dist/typings/index.d.ts"
  },
  "dependencies": {
    "@graphql-tools/compat": "0.0.1-alpha-33fb2950.0",
    "graphql-jit": "^0.7.0",
    "lru-cache": "^6.0.0"
  },
  "devDependencies": {
    "@graphql-tools/graphql": "0.1.0-alpha-33fb2950.0",
    "graphql": "^16.5.0",
    "graphql-jit": "0.7.3",
    "reflect-metadata": "0.1.13",
    "typescript": "4.7.4"
  },
  "peerDependencies": {
    "@envelop/core": "^2.4.2",
    "@graphql-tools/graphql": "0.1.0-alpha-33fb2950.0",
    "graphql": "^14.0.0 || ^15.0.0 || ^16.0.0"
  },
  "buildOptions": {
    "input": "./src/index.ts"
  },
  "publishConfig": {
    "directory": "dist",
    "access": "public"
  },
  "type": "module"
}
